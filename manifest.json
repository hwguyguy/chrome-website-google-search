{
	"manifest_version": 2,
	"name": "W Google Search",
	"description": "Better Google Search",
	"version": "1.0.0",
	"content_scripts": [
		{
			"matches": [
				"https://www.google.com/search?*",
				"https://www.google.com.hk/search?*"
			],
			"css": ["google.css"],
			"js": ["google.js"],
			"run_at": "document_idle"
		}
	]
}
